{% style %}
          .nxt-singleCourse-contents__area {
            background-color: {{ section.settings.background_color }};
          }
          .nxt-singleCourse__wrapper {
            padding-top: {{ section.settings.padding_top | times: 0.6 | round: 0 }}px;
            padding-bottom: {{ section.settings.padding_bottom | times: 0.6 | round: 0 }}px;
            padding-left: 15px;
            padding-right: 15px;
            border-top-left-radius: 48px;
            border-top-right-radius: 48px;
            box-shadow: 0px 1px 0px 0px #272C3166 inset;
            background-color: inherit;
            margin-top: -40px;
            position: relative;
            z-index: 10;
          }
        @media (min-width: 768px) {
            .nxt-singleCourse__wrapper {
                padding-top: {{ section.settings.padding_top }}px;
                padding-bottom: {{ section.settings.padding_bottom }}px;
            }
        }
          .nxt-singleCourse h2 {
              margin: 0;
              color: #FFFFFF;
              font-family: "Neue Haas Grotesk Display Pro", sans-serif;
              font-weight: 300;
              font-size: 48px;
              line-height: 54.72px;
          }

          .nxt-singleCourse p {
              color: #FFFFFF;
              font-family: "Inter", sans-serif;
              font-weight: 400;
              font-size: 16px;
              line-height: 24px;
          }
          .nxt-singleCourseMeta__wrapper {
            display: grid;
            grid-template-columns: repeat(5, 1fr);
            align-items: center;
            border: 1px solid #133B32;
            background: #041E18;
            padding: 25px 40px;
            border-radius: 8px;
            margin-bottom: 49px;
        }

        .courseMeta__box:not(:first-child){
            border-left: 1px solid #133B32;
            padding-left: 25px;
        }
        .nxt-card__stars > svg {
            width: 80px;
          }
        .nxt-card__stars {
          display: flex;
          gap: 4px;
          align-items: center;
          }
          .nxt-card__stars .rating__background {
            fill: #bfc3ca;
            stroke: red;
            stroke-width: 1;
            height: 100%;
            width: 100%;
          }

          .nxt-card__stars .rating__value {
            fill: #FFFFFF;
            height: 100%;
          }
          .courseMeta__box-title {
            font-family: 'Inter', sans-serif;
            font-weight: 400;
            font-size: 16px;
            line-height: 24px;
            color: #FFFFFF;
            margin: 0;
          }
          .courseMeta__box-description {
            font-family: 'Inter', sans-serif;
            font-weight: 400;
            font-size: 14px;
            line-height: 20px;
            color: #676C6B;
            margin: 0;
            white-space: nowrap;
          }

          .nxt-singleCourse__contents-wrapper {
          display: flex;
          gap: 20px;
          justify-content: space-between;
      }
      .nxt-singleCourse__contents-left {
          max-width: 818px;
          width: 100%;
      }
      .nxt-singleCourse__contents-right{
          max-width: 457px;
          width: 100%;
      }
      .nxt-contentBox:not(:last-child) {
          margin-bottom: 32px;
      }
      .nxt-iconBox {
        display: flex;
        align-items: center;
        gap: 16px;
        margin-top: 24px;
    }
    .nxt-iconBox-icon {
        display: flex;
        max-width: 32px;
    }
    .nxt-iconBox-icon img {
        max-width: 100%;
        height: auto;
        object-fit: contain;
    }
    .nxt-iconBox-title,
    .nxt-iconBox-description{
        color: #FFFFFF;
        font-family: "Inter", sans-serif;
        font-weight: 400;
        font-size: 16px;
        line-height: 24px;
        margin: 0;
    }
    .nxt-iconBox .nxt-iconBox-description{
        color: #676C6B;
    }
      .nxt-contentBox .nxt-tagList {
          margin:  24px 0 0;
          padding: 0;
          display: flex;
          align-items: center;
          flex-wrap: wrap;
          gap: 8px;
          list-style: none;
      }
      .nxt-contentBox .nxt-tagList li {
          background: #FFFFFF;
          font-family: "Inter", sans-serif;
          font-weight: 400;
          font-size: 16px;
          line-height: 24px;
          color: #020A08;
          padding: 8px 16px;
          border-radius: 5000px;
      }
      .nxt-contentBox .nxt-tagList a{
          font-family: "Source Sans 3", sans-serif;
          font-weight: 600;
          font-size: 14px;
          line-height: 20px;
          color: #FFFFFF;
      }

        .nxt-career__card {
                  padding: 24px 32px;
                  background: #041E18;
                  border-radius: 12px;
                  outline: 1px #133B32 solid;
                  outline-offset: -1px;
                  display: flex;
                  justify-content: space-between;
                  align-items: center;
                  gap: 24px;
                  margin-top: 89px;
              }

              .nxt-career__content {
                  flex: 1;
                  display: flex;
                  flex-direction: column;
                  gap: 24px;
              }

              .nxt-career__features {
                  display: flex;
                  flex-direction: column;
                  gap: 8px;
                  max-width: 482px;
                  color: white;
                  font-size: 16px;
                  font-family: "Inter Variable", Arial, sans-serif;
                  font-weight: 400;
                  line-height: 24px;
              }
  .nxt-career__features ul{
      padding: 0;
      margin: 0;
      padding-left: 20px;
  }
  .nxt-career__features ul li:not(:last-child){
      margin-bottom: 8px;
  }

              .nxt-career__cta {
                  display: inline-flex;
                  align-items: center;
                  gap: 8px;
                  text-decoration: none;
              }

              .nxt-career__cta-text {
                  background: linear-gradient(0deg, #36F4A4, #36F4A4),
                      linear-gradient(0deg, rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.2));
                  -webkit-background-clip: text;
                  background-clip: text;
                  -webkit-text-fill-color: transparent;
                  text-fill-color: transparent;
                  font-size: 12px;
                  font-family: Consolas, monospace;
                  font-weight: 400;
                  text-transform: uppercase;
                  line-height: 16px;
                  word-wrap: break-word;
              }

              .nxt-career__arrow {
                  display: flex;
              }

              .nxt-career__image {
                display: flex;
                  max-width: 511px;
                  border-radius: 8px;
                  overflow: hidden;
                  position: relative;
              }

              .nxt-career__image img {
                  max-width: 100%;
                  height: auto;
                  object-fit: contain;
                  border-radius: 8px;
              }
          @media (max-width: 1399px) {
            .nxt-singleCourseMeta__wrapper {
              padding: 20px;
            }
            .courseMeta__box-title {
              font-size: 14px;
            }
            .courseMeta__box-description {
              font-size: 12px;
            }
            .courseMeta__box:not(:first-child) {
              padding-left: 15px;
            }
          }
          @media (max-width: 1140px) {
            .nxt-singleCourseMeta__wrapper {
              grid-template-columns: repeat(2, 1fr);
            }
            .courseMeta__box-description {
              font-size: 12px;
              white-space: normal;
            }
            .courseMeta__box:not(:last-child) {
              border-bottom: 1px solid #133B32;
              margin-bottom: 10px;
              padding-bottom: 10px;
            }
            .courseMeta__box:not(:first-child) {
              border-left: none;
              padding-left: 0;
            }
          }
          @media (max-width: 1024px) {
            .nxt-career__card,
            .nxt-singleCourse__contents-wrapper {
              flex-direction: column;
              margin-top: 60px;
            }
            .nxt-singleCourse__contents-left,
            .nxt-singleCourse__contents-right {
              max-width: 100%;
            }
            .nxt-career__image {
              max-width: 100%;
            }
          }
          @media (max-width: 767px) {
            .nxt-singleCourseMeta__wrapper {
              margin-bottom: 30px;
            }
              .nxt-singleCourse h2 {
                  font-size: 20px;
                  line-height: 28px;
              }
              .nxt-singleCourse p {
                font-size: 12px;
                line-height: 18px;
              }

              .nxt-career__card {
                    padding: 15px;
                    margin-top: 30px;
              }

              .nxt-career__image {
                    width: 100%;
                    max-width: 400px;
                }
                .nxt-contentBox:not(:last-child) {
                  margin-bottom: 20px;
                }
                .nxt-iconBox {
                  margin-top: 16px;
                }
                .nxt-iconBox-title,
                .nxt-iconBox-description {
                  font-size: 12px;
                  line-height: 18px;
                }
                .nxt-iconBox-icon {
                  max-width: 24px;
                }
                .nxt-contentBox .nxt-tagList {
                  margin-top: 16px;
                }
                .nxt-contentBox .nxt-tagList li {
                  font-size: 12px;
                  line-height: 18px;
                  padding: 4px 8px;
                }
                .nxt-contentBox .nxt-tagList a {
                  font-size: 12px;
                  line-height: 18px;
                }
                .nxt-career__features {
                  font-size: 14px;
                  line-height: 20px;
                }

              }
          @media (max-width: 540px) {
            .nxt-singleCourseMeta__wrapper {
              padding: 15px;
              grid-template-columns: 1fr;
            }

          }
{% endstyle %}
<div class="nxt-singleCourse__wrapper">
  <div
    class="nxt-container{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}"
  >
    {% assign course_series = product.metafields.custom.course_series | default: '5 course series' %}

    {% for block in section.blocks %}
      {% if block.type == 'meta_card' %}
        <div class="nxt-singleCourseMeta__wrapper">
          {% if block.settings.course_series_show %}
            <div class="courseMeta__box">
              <div class="courseMeta__box-heading">
                <h2 class="courseMeta__box-title">
                  {% if course_series != blank %}
                    {{ course_series }}
                  {% else %}
                    {{ block.settings.course_series_title }}
                  {% endif %}
                </h2>
                <p class="courseMeta__box-description">
                  {{ block.settings.course_series_subtitle }}
                </p>
              </div>
            </div>
          {% endif %}
          {% if block.settings.course_rating_show %}
            <div class="courseMeta__box">
              <div class="courseMeta__box-heading">
                <h2 class="courseMeta__box-title">
                  {%- assign rating_value = product.metafields.custom.rating | default: 5 -%}
                  <div class="nxt-card__stars">
                    <svg viewBox="0 0 1000 200" class="rating">
                      <defs>
                        <polygon id="star"
                          points="100,0 131,66 200,76 150,128 162,200 100,166 38,200 50,128 0,76 69,66 ">
                        </polygon>
                        <clipPath id="stars">
                          <use xlink:href="#star"></use>
                          <use xlink:href="#star" x="20%"></use>
                          <use xlink:href="#star" x="40%"></use>
                          <use xlink:href="#star" x="60%"></use>
                          <use xlink:href="#star" x="80%"></use>
                        </clipPath>
                      </defs>
                      <rect class="rating__background" clip-path="url(#stars)"></rect>
                      <rect width="{{ rating_value | times: 20 }}%" class="rating__value" clip-path="url(#stars)"></rect>
                    </svg>
                    {{ rating_value }}
                  </div>
                </h2>
                <p class="courseMeta__box-description">
                  ( {{ product.metafields.custom.rating_count | default: '216,623' }} reviews)
                </p>
              </div>
            </div>
          {% endif %}
          {% if block.settings.course_level_show %}
            <div class="courseMeta__box">
              <div class="courseMeta__box-heading">
                <h2 class="courseMeta__box-title">
                  {{ product.metafields.custom.course_level | default: 'Beginner level' }}
                </h2>
                <p class="courseMeta__box-description">
                  {{ block.settings.course_level_subtitle }}
                </p>
              </div>
            </div>
          {% endif %}
          {% if block.settings.course_schedule_show %}
            <div class="courseMeta__box">
              <div class="courseMeta__box-heading">
                <h2 class="courseMeta__box-title">
                  {{ product.metafields.custom.course_schedule | default: 'Flexible schedule' }}
                </h2>
                <p class="courseMeta__box-description">
                  {{ product.metafields.custom.course_duration | default: '2 months at 10 hours a week' }}
                </p>
              </div>
            </div>
          {% endif %}
          {% if block.settings.career_credential_show %}
            <div class="courseMeta__box">
              <div class="courseMeta__box-heading">
                <h2 class="courseMeta__box-title">
                  {{ block.settings.career_credential_title }}
                </h2>
                <p class="courseMeta__box-description">
                  {{ block.settings.career_credential_subtitle }}
                </p>
              </div>
            </div>
          {% endif %}
        </div>
      {% endif %}
    {% endfor %}

    <div class="nxt-singleCourse__contents nxt-singleCourse">
      <div class="nxt-singleCourse__contents-wrapper">
        <div class="nxt-singleCourse__contents-left">
          {% for block in section.blocks %}
            {% if block.type == 'product_description' %}
              <div class="nxt-contentBox">
                {{ product.description }}
              </div>
            {% endif %}

            {% if block.type == 'whats_included_card' %}
              {% assign what_s_included = product.metafields.custom.what_s_included.value %}
              {% if what_s_included %}
                <div class="nxt-contentBox">
                  <h2>{{ block.settings.title }}</h2>
                  {% for item in what_s_included %}
                    {% assign title = item.title.value %}
                    {% assign sub_title = item.sub_title.value %}
                    {% assign icon = item.image.value %}

                    <div class="nxt-iconBox">
                      <div class="nxt-iconBox-icon">
                        <img src="{{ icon | image_url: width: 32 }}" alt="{{ title }}" width="32" height="32">
                      </div>
                      <div class="nxt-iconBox-content">
                        <h3 class="nxt-iconBox-title">
                          {{ title }}
                        </h3>
                        <p class="nxt-iconBox-description">
                          {{ sub_title }}
                        </p>
                      </div>
                    </div>
                  {% endfor %}
                </div>
              {% endif %}
            {% endif %}
          {% endfor %}
        </div>
        <div class="nxt-singleCourse__contents-right">
          {% for block in section.blocks %}
            {% if block.type == 'skills_you_ll_gain_card' %}
              <div class="nxt-contentBox">
                <h2>{{ block.settings.title }}</h2>
                <ul class="nxt-tagList">
                  {% for skill in product.metafields.custom.skills_you_ll_gain.value %}
                    <li>{{ skill }}</li>
                  {% endfor %}

                  <a href="{{ block.settings.button_link }}"> {{ block.settings.button_text }} </a>
                </ul>
              </div>
            {% endif %}
            {% if block.type == 'tools_you_ll_learn_card' %}
              <div class="nxt-contentBox">
                <h2>{{ block.settings.title }}</h2>
                <ul class="nxt-tagList">
                  {% for tool in product.metafields.custom.tools_you_ll_learn.value %}
                    <li>{{ tool }}</li>
                  {% endfor %}

                  <a href="{{ block.settings.button_link }}"> {{ block.settings.button_text }} </a>
                </ul>
              </div>
            {% endif %}
          {% endfor %}
        </div>
      </div>
      {% for block in section.blocks %}
        {% if block.type == 'career_card' %}
          <div class="nxt-career__card">
            <div class="nxt-career__content">
              <h2 class="nxt-career__title">{{ block.settings.title }}</h2>
              <div class="nxt-career__features">
                {% if product.metafields.custom.careers_details_list.value %}
                  <ul>
                    {% for feature in product.metafields.custom.careers_details_list.value %}
                      <li>{{ feature }}</li>
                    {% endfor %}
                  </ul>
                {% else %}
                  {{ block.settings.description }}
                {% endif %}
              </div>
              {% if product.metafields.custom.career_cta.value.url != blank %}
                {% assign career_cta = product.metafields.custom.career_cta.value.url %}
              {% else %}
                {% assign career_cta = block.settings.button_link %}
              {% endif %}

              {% if career_cta != blank %}
                <a href="{{ career_cta }}" class="nxt-career__cta">
                  <span class="nxt-career__cta-text">{{ block.settings.button_text }}</span>
                  <div class="nxt-career__arrow">
                    <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
