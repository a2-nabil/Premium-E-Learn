{% style %}
    .rh-faq-section{
  background: {{ section.settings.background_color | default: '#020a08' }};
  padding: {{ section.settings.padding_top_desktop | default: 128 }}px {{ section.settings.padding_horizontal_desktop | default: 20 }}px {{ section.settings.padding_bottom_desktop | default: 128 }}px {{ section.settings.padding_horizontal_desktop | default: 20 }}px;
  position: relative;
  overflow: hidden;
}
.rh-faq-section .container_rh{
  max-width: 1420px;
  margin: 0 auto;
  width: 100%;
  position: relative;
  z-index: 2;
}
.rh-faq-section .rh-faq-overlay-section{
  position: absolute;
  top: 114px;
  left: 58px;
  width: 1804px;
  height: 907px;
  z-index: 1;
}
.rh-faq-section .rh-faq-overlay-section img{
  width: 100%;
  height: 100%;
  object-fit: cover;
}
 .rh-faq-section .rh-faq-sec-title{
   color: #fff;
   font-size: 50px;
   font-family: 'Inter', sans-serif;
   font-weight: 400;
   line-height: 60px;
   word-wrap: break-word;
   text-align: left;
   margin: 0 0 64px;
   width: 100%;
 }

 .rh-faq-sec-content-wrapper {
   width: 100%;
 }

 .rh-faq-list_rh {
   display: flex;
   flex-direction: column;
   justify-content: flex-start;
   align-items: flex-start;
   gap: 24px;
   width: 100%;
 }

 .rh-faq-item_rh {
   display: flex;
   flex-direction: column;
   justify-content: flex-start;
   align-items: flex-start;
   width: 100%;
   padding-bottom: 24px;
   border-bottom: 1px solid rgba(255, 255, 255, 0.30);
 }

 .rh-faq-item_rh:last-child {
   border-bottom: none;
 }

 .rh-faq-question-wrapper_rh {
   display: flex;
   justify-content: flex-start;
   align-items: center;
   gap: 16px;
   width: 100%;
   cursor: pointer;
   transition: all 0.3s ease;
 }

 .rh-faq-question-wrapper_rh:hover {
   opacity: 0.8;
 }

 .rh-faq-question-content_rh {
   flex: 1;
   display: flex;
   flex-direction: column;
   justify-content: flex-start;
   align-items: flex-start;
   max-width: 768px;
   padding-bottom: 0.59px;
 }

 .rh-faq-question_rh {
   color: white;
   font-size: 26px;
   font-family: 'Inter Variable', 'Inter', Arial, sans-serif;
   font-weight: 400;
   line-height: 33.60px;
   word-wrap: break-word;
   margin: 0;
 }

 .rh-faq-toggle-button_rh {
   flex-shrink: 0;
   display: flex;
   flex-direction: column;
   justify-content: flex-start;
   align-items: flex-end;
   margin-left: auto;
 }

 .rh-faq-icon_rh {
   width: 44px;
   height: 44px;
   background: white;
   border-radius: 50%;
   display: flex;
   justify-content: center;
   align-items: center;
   padding: 12px;
   transition: all 0.3s ease;
 }

 .rh-faq-item_rh.active .rh-faq-icon_rh {
   background: #36F4A4;
   transform: rotate(0deg);
 }

 .rh-faq-icon-horizontal_rh,
 .rh-faq-icon-vertical_rh {
  display: block !important;
   background: black;
   transition: all 0.3s ease;
 }

 .rh-faq-icon-horizontal_rh {
   width: 16px;
   height: 2px;
 }

 .rh-faq-icon-vertical_rh {
   width: 2px;
   height: 16px;
   position: absolute;
 }

 .rh-faq-item_rh.active .rh-faq-icon-vertical_rh {
   opacity: 0;
 }

 .rh-faq-answer_rh {
   width: 100%;
   max-height: 0;
   overflow: hidden;
   transition: max-height 0.3s ease-out;
   margin-top: 12px;
 }

 .rh-faq-answer_rh p {
   color: rgba(255, 255, 255, 0.8);
   font-size: 18px;
   font-family: 'Inter', Arial, sans-serif;
   font-weight: 400;
   line-height: 26px;
   margin: 0;
   padding: 12px 0;
 }

