      "type": "rh-faq-section",
      "blocks": {
        "faq_item_PxptBd": {
          "type": "faq_item",
          "settings": {
            "question": "What's included in the free trial?",
            "answer": "The free trial includes access to our complete learning platform for 7 days, including video courses, interactive exercises, downloadable resources, and community forums. You can explore any course during this period."
          }
        },
        "faq_item_hqPnHw": {
          "type": "faq_item",
          "settings": {
            "question": "Can I get a printed certificate?",
            "answer": "Yes! Upon course completion, you'll receive a digital certificate that you can download and print. We also offer premium printed certificates that can be shipped to your address for an additional fee."
          }
        },
        "faq_item_VcTbwz": {
          "type": "faq_item",
          "settings": {
            "question": "Is this recognised by employers?",
            "answer": "Absolutely! Our certifications are recognized by over 500+ companies worldwide. We're accredited by leading industry organizations and our certificates hold real value in the job market."
          }
        },
        "faq_item_iRREGh": {
          "type": "faq_item",
          "settings": {
            "question": "Do you offer job placement support?",
            "answer": "Yes, we provide comprehensive job placement support including resume reviews, interview preparation, career counseling, and access to our exclusive job board with partner companies."
          }
        },
        "faq_item_3zTgyi": {
          "type": "faq_item",
          "settings": {
            "question": "How fast will I receive my certificate?",
            "answer": "Digital certificates are issued immediately upon course completion. You'll receive an email with your certificate within minutes of finishing your final assessment."
          }
        },
        "faq_item_Fi6z3J": {
          "type": "faq_item",
          "settings": {
            "question": "Will the Study Hub content be updated for syllabus changes?",
            "answer": "Yes, our content is regularly updated to reflect industry changes and new developments. You'll always have access to the most current and relevant learning materials."
          }
        }
      },
